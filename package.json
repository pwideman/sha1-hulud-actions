{
  "name": "find-sha1-hulud-users",
  "version": "0.1.0",
  "description": "Custom actions to look for potential Sha1-Hulud activity related to your GitHub Enterprise",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "bundle": "rollup -c rollup.config.ts --configPlugin typescript",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src __tests__",
    "lint:fix": "eslint src __tests__ --fix",
    "format": "prettier --write src __tests__",
    "format:check": "prettier --check src __tests__",
    "all": "npm run format:check && npm run lint && npm run test && npm run bundle"
  },
  "keywords": [
    "github",
    "action",
    "sha1-hulud"
  ],
  "author": "Paul Wideman",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pwideman/find-sha1-hulud-users.git"
  },
  "bugs": {
    "url": "https://github.com/pwideman/find-sha1-hulud-users/issues"
  },
  "homepage": "https://github.com/pwideman/find-sha1-hulud-users#readme",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@actions/core": "^1.11.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@vitest/coverage-v8": "^4.0.14",
    "eslint": "^9.39.1",
    "prettier": "^3.7.1",
    "rollup": "^4.53.3",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vitest": "^4.0.14"
  }
}
